<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.pusher.feeds.sample.MainActivity"
    android:orientation="vertical">

    <Button
        android:id="@+id/subscribe_public_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Subscribe public"/>

    <Button
        android:id="@+id/subscribe_private_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Subscribe private"/>

    <Button
        android:id="@+id/unsubscribe_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Unsubscribe"/>

    <Button
        android:id="@+id/list_feeds_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="List Feeds"/>

    <Button
        android:id="@+id/paginate_public_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Paginate public"/>

    <Button
        android:id="@+id/paginate_private_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Subscribe private"/>




</LinearLayout>
